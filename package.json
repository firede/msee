{
  "name": "msee",
  "version": "0.3.2",
  "description": "Msee is a command-line tool to read Markdown file in your terminal, and it's a library help your command-line software to output readable markdown content.",
  "main": "./lib/msee.js",
  "bin": "./bin/msee",
  "scripts": {
    "test": "env FORCE_COLOR=true tap --lines=85 test/**/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/firede/msee.git"
  },
  "dependencies": {
    "ansi-regex": "^2.0.0",
    "ansicolors": "^0.3.2",
    "cardinal": "^0.7.1",
    "chalk": "^1.1.1",
    "marked": "^0.3.5",
    "nopt": "^3.0.4",
    "table-header": "^0.2.2",
    "text-table": "^0.2.0",
    "wcstring": "^2.1.0",
    "xtend": "^4.0.0"
  },
  "keywords": [
    "markdown",
    "terminal",
    "reader",
    "console",
    "shell",
    "colors",
    "highlight"
